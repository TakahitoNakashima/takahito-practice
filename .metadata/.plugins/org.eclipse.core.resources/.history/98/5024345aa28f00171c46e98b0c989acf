class answer {
	private String name;
	
	answer() {
		this.name = "no name";
	}
	answer(String name) {
		this.name = name;
	}
	
	public static void main(String args[]) {
		String withoutName = answer().name;
		String withName = answer("Takahito").name;
		
		System.out.println(withoutName);
		System.out.println(withName);
	}
	
	
}